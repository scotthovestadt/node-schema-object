    "babel-plugin-transform-class-properties": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-mocha": "2.2.0",
    "gulp-mocha-phantomjs": "0.11.0",
    "load-plugins": "2.1.0",
    "minimist": "1.2.0",
    "should": "8.x.x"
  },
